{
  "version": "2.0",

  "extensions": {
    "durableTask": {
      "hubName": "TestHubName",
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage"
      }
    }
  },

  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}
